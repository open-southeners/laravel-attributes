{
    "name": "open-southeners/laravel-attributes",
    "description": "Extends the Laravel framework with useful PHP 8 attributes",
    "license": "MIT",
    "keywords": [
        "open-southeners"
    ],
    "authors": [
        {
            "name": "Ruben Robles",
            "email": "d8vjork@outlook.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0|^8.1"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/larastan": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^7.0|^9.0",
        "orchestra/testbench": "^4.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "OpenSoutheners\\LaravelAttributes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenSoutheners\\LaravelAttributes\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenSoutheners\\LaravelAttributes\\JsonApiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
